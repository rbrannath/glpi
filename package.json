    "scripts": {
        "ddev:curl": "sudo curl -fsSL https://pkg.ddev.com/apt/gpg.key | gpg --dearmor | sudo tee /usr/share/keyrings/ddev.gpg > /dev/null",
        "ddev:deb": "echo \"deb [signed-by=/usr/share/keyrings/ddev.gpg] https://pkg.ddev.com/apt/ stable main\" | sudo tee /etc/apt/sources.list.d/ddev.list >/dev/null",
